//Use Of ScannerClass to take a integer Input
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
   
      
     System.out.println("Enter Your Age");  
    Scanner sc = new Scanner(System.in);
    int a =sc.nextInt();
      System.out.println("Your Age is "+a);  
      
      
      
      
        
        
    }
}
